<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.login.MenuPage">
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen50">
        <TextView
            android:id="@+id/btn_Close"
            android:layout_width="@dimen/dimen100"
            android:layout_height="@dimen/dimen45"
            android:text="@string/Close"
            android:textColor="@color/colorPrimar"
            android:textSize="@dimen/dimen25"
            android:gravity="center_horizontal"
            android:layout_marginLeft="@dimen/dimen400"/>
        <TextView
            android:layout_width="@dimen/dimen200"
            android:layout_height="@dimen/dimen50"
            android:layout_centerHorizontal="true"
            android:text="@string/MenuPage"
            android:gravity="center_horizontal"
            android:textSize="@dimen/dimen25"
            android:textColor="@color/colorText"/>

    </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dimen35"
        android:layout_alignParentTop="true"
        android:scrollbars="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/user_Info"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_User"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen9"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:onClick="clickMe"
                    android:text="@string/UserInfo"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/basic_Info"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Basic"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:onClick="clickMe"
                    android:text="@string/BasicInfo"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />

                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen25"
                    android:layout_marginLeft="@dimen/dimen450"
                    android:layout_marginTop="@dimen/dimen15"
                    android:src="@drawable/icon_continue" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/badge_Collection"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Badge"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:onClick="clickMe"
                    android:text="@string/BadgeCollection"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen25"
                    android:layout_marginLeft="@dimen/dimen450"
                    android:layout_marginTop="@dimen/dimen15"
                    android:src="@drawable/icon_continue" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/new_User"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_NewUser"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:onClick="clickMe"
                    android:text="@string/NewUserRegisretion"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen25"
                    android:layout_marginLeft="@dimen/dimen450"
                    android:layout_marginTop="@dimen/dimen15"
                    android:src="@drawable/icon_continue" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/login"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Login"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/Login"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen25"
                    android:layout_marginLeft="@dimen/dimen450"
                    android:layout_marginTop="@dimen/dimen15"
                    android:src="@drawable/icon_continue" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/logout"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Logout"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/Logout"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/app_Info"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_AppInfo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/AppInfo"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/how_To"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_HowTo"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/HowToUse"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dimen25"
                    android:layout_marginLeft="@dimen/dimen450"
                    android:layout_marginTop="@dimen/dimen15"
                    android:src="@drawable/icon_continue" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/privacy"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Privacy"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:onClick="clickMe"
                    android:text="@string/PrivacyPolicy"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/inquiry"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Inquiry"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/Inquiry"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rateInApp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_RateInApp"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/RateInApp"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/version"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:id="@+id/btn_Version"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dimen10"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/Version"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25"/>
            </RelativeLayout>

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="@dimen/dimen60">
                <TextView
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/dimen5"
                    android:layout_marginTop="@dimen/dimen10"
                    android:layout_marginLeft="@dimen/dimen10"
                    android:text="@string/versiontwo"
                    android:onClick="clickMe"
                    android:textColor="@color/colorText"
                    android:textSize="@dimen/dimen25" />
            </RelativeLayout>

        </LinearLayout>

    </ScrollView>

</RelativeLayout>