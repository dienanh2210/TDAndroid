<?xml version="1.0" encoding="UTF-8"?>

    <set android:interpolator="@android:anim/linear_interpolator"
    android:fillAfter="true"
    xmlns:android="http://schemas.android.com/apk/res/android">

    <alpha android:interpolator="@android:anim/accelerate_interpolator"
        android:repeatMode="reverse"
        android:duration="100"
        android:toAlpha="1.0"
        android:fromAlpha="0.0"/>

    <rotate android:interpolator="@android:anim/linear_interpolator"
    android:repeatCount="1"
    android:repeatMode="reverse"
    android:duration="150"
    android:toDegrees="-15"
    android:startOffset="0"
    android:pivotY="50%"
    android:pivotX="50%"
    android:fromDegrees="0"/>
    <scale
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:duration="300"
        android:fromXScale="0.6"
        android:fromYScale="0.6"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toXScale="1"
        android:toYScale="1" >
    </scale>
